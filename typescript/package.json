{
  "name": "lift-pass-pricing",
  "version": "1.0.0",
  "description": "Lift-Pass-Pricing refactoring kata",
  "main": "index.js",
  "scripts": {
    "start": "tsc && node .",
    "test": "nyc node_modules/.bin/mocha -r ts-node/register -r source-map-support/register --recursive test/**/*.spec.ts --timeout 15000",
    "report": "serve -s coverage -p 8081",
    "compile": "tsc"
  },
  "readme": "README.md",
  "license": "ISC",
  "dependencies": {
    "@types/chai": "^4.1.7",
    "@types/express": "^4.16.1",
    "@types/mocha": "^5.2.6",
    "@types/supertest": "^2.0.7",
    "@types/supertest-as-promised": "^2.0.37",
    "chai": "^4.2.0",
    "express": "^4.16.4",
    "mocha": "^6.1.2",
    "mysql2": "^1.6.5",
    "supertest": "^4.0.2",
    "supertest-as-promised": "^4.0.2",
    "ts-node": "^8.0.3",
    "typescript": "^3.4.3"
  },
  "devDependencies": {
    "nyc": "^15.1.0",
    "serve": "^13.0.2",
    "source-map-support": "^0.5.21",
    "testcontainers": "^8.4.0"
  },
  "nyc": {
    "all": true,
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "html"
    ],
    "report-dir": "coverage"
  }
}
